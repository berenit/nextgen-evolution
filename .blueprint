models:
    Ip: { ip_class_id: integer, address: string, hostname: 'text nullable' }
    IpClass: { cidr: string, label: 'string nullable', vlan_id: integer }
    User: { name: string, email: string, email_verified_at: 'datetime nullable', password: string, remember_token: 'string nullable', two_factor_secret: 'text nullable', two_factor_recovery_codes: 'text nullable', two_factor_confirmed_at: 'datetime nullable' }
    Vlan: { address: string }
    vLan: { vlan: integer, description: string }
    ipClass: { cidr: string, description: string, relationships: { belongsTo: Vlan } }
    privateIp: { ip: string, description: string, macAddress: string, relationships: { belongsTo: IpClass, hasMany: Hostname } }
    hostname: { name: string, relationships: { belongsTo: PrivateIp } }
    publicIP: { ip: string, relationships: { hasMany: Hostname } }
    publicIp: { ip: string, relationships: { hasMany: Hostname } }
    vlan: { vlan: integer, description: string }
