created:
    - database/factories/VlanFactory.php
    - database/factories/IpClassFactory.php
    - database/factories/PrivateIpFactory.php
    - database/factories/HostnameFactory.php
    - database/factories/PublicIpFactory.php
    - database/migrations/2026_01_02_234156_create_vlans_table.php
    - database/migrations/2026_01_02_234157_create_ip_classes_table.php
    - database/migrations/2026_01_02_234158_create_private_ips_table.php
    - database/migrations/2026_01_02_234159_create_hostnames_table.php
    - database/migrations/2026_01_02_234200_create_public_ips_table.php
